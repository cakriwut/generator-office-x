{
  "name": "generator-office-x",
  "version": "1.0.0",
  "description": "Extended Office AddIns Yeoman Generator",
  "homepage": "https://github.com/cakriwut/generator-office-x",
  "author": {
    "name": "Riwut Libinuko",
    "email": "cakriwut@gmail.com",
    "url": "https://blog.libinuko.com"
  },
  "files": [
    "app",
    "generators"
  ],
  "main": "app/index.js",
  "keywords": [
    "office",
    "yeoman-generator",
    "office-add-in",
    "add-in",
    "outlook",
    "excel",
    "word",
    "powerpoint",
    "onenote",
    "project",
    "vuejs",
    "extra",
    "extended"
  ],
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-config-xo-space": "^0.21.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "prettier": "^1.15.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 6.0.0"
  },
  "dependencies": {
    "applicationinsights": "^1.1.0",
    "camaro": "^3.0.14",
    "chalk": "^2.1.0",
    "command-exists": "^1.2.8",
    "generator-office": "^1.1.24",
    "merge-package-json": "^0.1.3",
    "path": "^0.12.7",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "templates"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo-space/esnext",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ],
      "no-negated-condition": "off"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint . --fix",
    "test": "jest"
  },
  "repository": "cakriwut/generator-office-x",
  "license": "MIT"
}
